@{ ViewData["Title"] = "Index"; }

<table>
    <thead>
        <tr>
            <th>Codigo Captura</th>
            <th>Nombre</th>
            <th>Tipo</th>
            <th>fecha</th>
            <th>Imagen</th>
            <th>Liberar</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model){
        <tr>
            <td>@item.idEntrenadorPokemon</td>
            <td>@item.pokemon.nombres</td>
            <td>@item.pokemon.tipo</td>
            <td>@item.fecha.ToString("dd/mm/yyyy")</td>
            <td>
                <img src="@item.pokemon.imagen" height="60px" height="60px">
            </td>
            <td>
                <a href="/EntrenadorPokemon/Delete?id=@item.idEntrenadorPokemon">Liberar</a>
            </td>
        </tr>
        }

    </tbody>
</table>